[:update
 {:table foo}
 [:project
  ({xt$iid foo.1/xt$iid}
   {xt$valid_from foo.1/xt$valid_from}
   {xt$valid_to foo.1/xt$valid_to}
   {bar
    (and
     (< foo.1/xt$system_from foo.1/xt$valid_to)
     (> foo.1/xt$system_to foo.1/xt$valid_from))}
   {baz foo.1/baz})
  [:rename
   foo.1
   [:scan
    {:table foo}
    [xt$valid_from
     xt$system_to
     xt$iid
     xt$valid_to
     baz
     xt$system_from]]]]]
