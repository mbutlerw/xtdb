[:project
 [{promo_revenue (/ (* 100.0 xt$sum_out_3) xt$sum_out_5)}]
 [:group-by
  [{xt$sum_out_3 (sum xt$sum_in_4)} {xt$sum_out_5 (sum xt$sum_in_6)}]
  [:map
   [{xt$sum_in_4
     (cond
      (like p.2/p_type "PROMO%")
      (* l.1/l_extendedprice (- 1 l.1/l_discount))
      0)}
    {xt$sum_in_6 (* l.1/l_extendedprice (- 1 l.1/l_discount))}]
   [:mega-join
    [{l.1/l_partkey p.2/p_partkey}]
    [[:rename
      l.1
      [:scan
       {:table lineitem}
       [l_partkey
        {l_shipdate
         (and
          (<
           l_shipdate
           (+
            #time/date "1995-09-01"
            (single-field-interval "1" "MONTH" 2 6)))
          (>= l_shipdate #time/date "1995-09-01"))}
        l_discount
        l_extendedprice]]]
     [:rename p.2 [:scan {:table part} [p_partkey p_type]]]]]]]]
